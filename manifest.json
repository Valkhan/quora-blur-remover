{
  "manifest_version": 3,
  "name": "Quora Blur Remover",
  "version": "1.1",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": [
        "https://quora.com/*",
        "https://*.quora.com/*"
      ],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "quorablurremover@valkhan.com.br"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "author": "Valkhan Tech"
}
